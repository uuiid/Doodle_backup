{
  "version": 3,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 20,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "Ninja_build",
      "displayName": "Ninja build",
      "description": "Ninja build using vcpkg",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build/${presetName}",
      "installDir": "${sourceDir}/build/install/$env{doodle_install_prefix}",
      "toolchainFile": "$env{VCPKG_ROOT}/scripts/buildsystems/vcpkg.cmake",
      "architecture": {
        "value": "x64",
        "strategy": "external"
      },
      "toolset": {
        "value": "v142,host=x64,version=14",
        "strategy": "external"
      },
      "cacheVariables": {
        "CMAKE_SYSTEM_VERSION": "10.0.22000.0",
        "CMAKE_C_COMPILER": "cl",
        "CMAKE_CXX_COMPILER": "cl"
      },
      "vendor": {
        "microsoft.com/VisualStudioSettings/CMake/1.0": {
          "hostOS": [
            "Windows"
          ],
          "disableExternalAnalysis": true
        }
      },
      "cmakeExecutable": "C:/Program Files/CMake/bin/cmake.exe",
      "hidden": true
    },
    {
      "name": "Ninja_RelWithDebInfo",
      "inherits": "Ninja_build",
      "displayName": "config maya plug",
      "description": "构建maya插件 默认版本是2019",
      "cacheVariables": {
        "Maya_Version": "2019",
        "CMAKE_BUILD_TYPE": "RelWithDebInfo"
      }
    },
    {
      "name": "Ninja_release",
      "inherits": "Ninja_build",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "Ue4_ROOT_DIR": "D:/Program Files/Epic Games/UE_4.27",
        "Photoshop_Version": "120.0",
        "ADD_VERSION_LABEL": true
      }
    },
    {
      "name": "Ninja_release_maya_2018",
      "inherits": "Ninja_release",
      "cacheVariables": {
        "Maya_Version": "2018"
      },
      "hidden": true
    },
    {
      "name": "Ninja_release_maya_2019",
      "inherits": "Ninja_release",
      "cacheVariables": {
        "Maya_Version": "2019"
      },
      "hidden": true
    },
    {
      "name": "Ninja_release_maya_2020",
      "inherits": "Ninja_release",
      "cacheVariables": {
        "Maya_Version": "2020"
      },
      "hidden": true
    },
    {
      "name": "Ninja_debug",
      "inherits": "Ninja_build",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "Ue4_ROOT_DIR": "D:/Program Files/Epic Games/UE_4.27",
        "Maya_Version": "2018",
        "Photoshop_Version": "120.0",
        "BUILD_LIMITED_1": "OFF",
        "BUILD_USD": "ON",
        "PXR_USD_ROOT": "E:/usd_install",
        "Boost_NO_WARN_NEW_VERSIONS": true
      }
    },
    {
      "name": "Ninja_debug_limited_1",
      "inherits": "Ninja_build",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "Ue4_ROOT_DIR": "D:/Program Files/Epic Games/UE_4.27",
        "Maya_Version": "2018",
        "Photoshop_Version": "120.0",
        "BUILD_LIMITED_1": "ON"
      }
    },
    {
      "name": "vs2022",
      "inherits": "Ninja_debug",
      "generator": "Visual Studio 17 2022"
    }
  ],
  "buildPresets": [
    {
      "name": "debug",
      "configurePreset": "Ninja_debug",
      "jobs": 10,
      "hidden": true
    },
    {
      "name": "debug_doodle",
      "description": "调试 exe",
      "displayName": "调试 doodle exe",
      "inherits": "debug",
      "targets": [
        "DoodleExe",
        "test_main"
      ]
    },
    {
      "name": "doxygen",
      "description": "doc",
      "displayName": "生成日志",
      "configurePreset": "Ninja_release",
      "targets": [
        "Doxygen"
      ]
    },
    {
      "name": "debug_maya_plug",
      "description": "调试 maya 插件(maya插件只能使用release调试)",
      "displayName": "调试 doodle maya",
      "configurePreset": "Ninja_RelWithDebInfo",
      "inherits": "debug",
      "targets": [
        "doodle_maya"
      ]
    },
    {
      "name": "debug_maya_exe",
      "description": "调试 maya 库连接的exe(maya只能使用release调试)",
      "displayName": "调试 doodle maya exe",
      "configurePreset": "Ninja_RelWithDebInfo",
      "inherits": "debug",
      "targets": [
        "DoodleExe",
        "doodle_maya_exe",
        "test_main"
      ]
    },
    {
      "name": "Release_Exe",
      "configurePreset": "Ninja_release",
      "jobs": 10,
      "targets": [
        "DoodleExe",
        "generate_token",
        "Doxygen"
      ]
    },
    {
      "name": "Release_Maya",
      "configurePreset": "Ninja_release",
      "jobs": 10,
      "targets": [
        "doodle_maya",
        "doodle_maya_exe"
      ]
    },
    {
      "name": "Release_maya_2018",
      "configurePreset": "Ninja_release_maya_2018",
      "jobs": 10,
      "targets": [
        "doodle_maya"
      ],
      "hidden": true
    },
    {
      "name": "Release_maya_2019",
      "configurePreset": "Ninja_release_maya_2019",
      "jobs": 10,
      "targets": [
        "doodle_maya"
      ],
      "hidden": true
    },
    {
      "name": "Release_maya_2020",
      "configurePreset": "Ninja_release_maya_2020",
      "jobs": 10,
      "targets": [
        "doodle_maya"
      ],
      "hidden": true
    }
  ],
  "testPresets": [
    {
      "name": "develop",
      "configurePreset": "Ninja_debug",
      "output": {
        "outputOnFailure": true
      },
      "execution": {
        "noTestsAction": "error",
        "stopOnFailure": true
      },
      "filter": {
        "include": {
          "name": "pinyi"
        }
      }
    },
    {
      "name": "test_bvh",
      "description": "测试bvh",
      "displayName": "bvh",
      "configurePreset": "Ninja_debug",
      "output": {
        "outputOnFailure": true
      },
      "filter": {
        "include": {
          "name": "bvh"
        }
      },
      "execution": {
        "noTestsAction": "error",
        "stopOnFailure": true
      }
    }
  ],
  "vendor": {}
}